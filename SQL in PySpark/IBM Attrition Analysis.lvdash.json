{
  "datasets": [
    {
      "name": "946d985f",
      "displayName": "DemographicalGraphs",
      "queryLines": [
        "SELECT \r\n",
        "        HR.DEPARTMENT,\r\n",
        "        /*HR.JOBROLE,\r\n",
        "        (CASE\r\n",
        "                WHEN HR.AGE < 25 THEN '18-24'\r\n",
        "                WHEN HR.AGE BETWEEN 25 AND 34 THEN '25-34'\r\n",
        "                WHEN HR.AGE BETWEEN 35 AND 44 THEN '35-44'\r\n",
        "                WHEN HR.AGE BETWEEN 45 AND 54 THEN '45-54'\r\n",
        "                ELSE '55+'\r\n",
        "        END) AS AgeGroup,*/\r\n",
        "        ROUND(\r\n",
        "                CAST(SUM(CASE WHEN HR.ATTRITION = 'Yes' THEN 1 ELSE 0 END) AS REAL) \r\n",
        "                / COUNT(*),\r\n",
        "        2) AttRatio\r\n",
        "\r\n",
        "        FROM workspace.default.ibm_hr HR\r\n",
        "        GROUP BY HR.DEPARTMENT--,HR.JOBROLE,AgeGroup"
      ]
    },
    {
      "name": "53e6e1fb",
      "displayName": "CardValues",
      "queryLines": [
        "SELECT  \r\n",
        "        COUNT(*) AS Total_Employees,\r\n",
        "        \r\n",
        "        SUM(CASE WHEN HR.ATTRITION = 'Yes' THEN 1 ELSE 0 END) AS Total_Attrition_Yes,\r\n",
        "\r\n",
        "        ROUND(\r\n",
        "        CAST(SUM(CASE WHEN HR.ATTRITION = 'Yes' THEN 1 ELSE 0 END) AS REAL) \r\n",
        "        / COUNT(*),\r\n",
        "        2\r\n",
        "    ) AS Attrition_Rate_Percentage\r\n",
        "        FROM workspace.default.ibm_hr HR"
      ]
    },
    {
      "name": "227eec53",
      "displayName": "OverTimeAtt",
      "queryLines": [
        "SELECT \r\n",
        "        HR.OVERTIME,\r\n",
        "        \r\n",
        "        ROUND(\r\n",
        "                CAST(SUM(CASE WHEN HR.ATTRITION = 'Yes' THEN 1 ELSE 0 END) AS REAL) \r\n",
        "                / COUNT(*),\r\n",
        "        2) AttRatio\r\n",
        "        FROM workspace.default.ibm_hr HR\r\n",
        "        GROUP BY HR.OVERTIME"
      ]
    },
    {
      "name": "21cdfbb6",
      "displayName": "BusinessTravelAtt",
      "queryLines": [
        "SELECT \r\n",
        "        REPLACE(HR.BUSINESSTRAVEL, '_', ' ') AS BusinessTravel,\r\n",
        "        \r\n",
        "        ROUND(\r\n",
        "                CAST(SUM(CASE WHEN HR.ATTRITION = 'Yes' THEN 1 ELSE 0 END) AS REAL) \r\n",
        "                / COUNT(*),\r\n",
        "        2) AttRatio\r\n",
        "        FROM workspace.default.ibm_hr HR\r\n",
        "        GROUP BY HR.BUSINESSTRAVEL"
      ]
    },
    {
      "name": "48525da8",
      "displayName": "JobRoleAtt",
      "queryLines": [
        "SELECT \r\n",
        "        \r\n",
        "        HR.JOBROLE,\r\n",
        "       /* (CASE\r\n",
        "                WHEN HR.AGE < 25 THEN '18-24'\r\n",
        "                WHEN HR.AGE BETWEEN 25 AND 34 THEN '25-34'\r\n",
        "                WHEN HR.AGE BETWEEN 35 AND 44 THEN '35-44'\r\n",
        "                WHEN HR.AGE BETWEEN 45 AND 54 THEN '45-54'\r\n",
        "                ELSE '55+'\r\n",
        "        END) AS AgeGroup,*/\r\n",
        "        ROUND(\r\n",
        "                CAST(SUM(CASE WHEN HR.ATTRITION = 'Yes' THEN 1 ELSE 0 END) AS REAL) \r\n",
        "                / COUNT(*),\r\n",
        "        2) AttRatio\r\n",
        "\r\n",
        "        FROM workspace.default.ibm_hr HR\r\n",
        "        GROUP BY HR.JOBROLE"
      ]
    },
    {
      "name": "32d7fe18",
      "displayName": "AgeRatioAtt",
      "queryLines": [
        "SELECT \r\n",
        "        HR.ATTRITION,\r\n",
        "        HR.AGE\r\n",
        "        \r\n",
        "\r\n",
        "        FROM workspace.default.ibm_hr HR\r\n",
        "       "
      ]
    },
    {
      "name": "d5351182",
      "displayName": "JobSatisfactionAtt",
      "queryLines": [
        "SELECT \r\n",
        "        HR.JOBSATISFACTION,\r\n",
        "        SUM(ROUND(CAST(SUM(CASE WHEN HR.ATTRITION = 'Yes' THEN 1 ELSE 0 END) AS REAL) / COUNT(*),2) ) OVER(PARTITION BY HR.JOBSATISFACTION) AttRatioJS\r\n",
        "FROM workspace.default.ibm_hr HR\r\n",
        "GROUP BY HR.JOBSATISFACTION"
      ]
    },
    {
      "name": "955a9454",
      "displayName": "MonthlyIncomeValues",
      "queryLines": [
        "SELECT \r\n",
        "        HR.MonthlyIncome,\r\n",
        "        HR.Attrition\r\n",
        "FROM workspace.default.ibm_hr HR\r\n"
      ]
    },
    {
      "name": "55a22e09",
      "displayName": "YearsWithAtt",
      "queryLines": [
        "SELECT \r\n",
        "       CASE WHEN HR.YearsAtCompany < 5 THEN '0-5'\r\n",
        "              WHEN HR.YearsAtCompany BETWEEN 5 AND 10 THEN '5-10'\r\n",
        "              WHEN HR.YearsAtCompany BETWEEN 10 AND 20 THEN '10-20'\r\n",
        "              WHEN HR.YearsAtCompany BETWEEN 20 AND 30 THEN '20-30'\r\n",
        "              WHEN HR.YearsAtCompany BETWEEN 30 AND 40 THEN '30-40'\r\n",
        "              WHEN HR.YearsAtCompany > 40 THEN '40+'\r\n",
        "              ELSE NULL END AS YearsAtCompany,\r\n",
        "\r\n",
        "       ROUND(\r\n",
        "                CAST(SUM(CASE WHEN HR.ATTRITION = 'Yes' THEN 1 ELSE 0 END) AS REAL) \r\n",
        "                / COUNT(*),\r\n",
        "        2) AttRatio\r\n",
        "      \r\n",
        "       /*HR.YearsWithCurrManager,\r\n",
        "       SUM(ROUND(CAST(SUM(CASE WHEN HR.ATTRITION = 'Yes' THEN 1 ELSE 0 END) AS REAL) / COUNT(*),2) ) OVER(PARTITION BY HR.YearsAtCompany) AttRatioYearsAtComp,\r\n",
        "       SUM(ROUND(CAST(SUM(CASE WHEN HR.ATTRITION = 'Yes' THEN 1 ELSE 0 END) AS REAL) / COUNT(*),2) ) OVER(PARTITION BY HR.YearsWithCurrManager) AttRatioYearsAtMan*/\r\n",
        "FROM workspace.default.ibm_hr HR\r\n",
        "GROUP BY YearsAtCompany\r\n",
        "\r\n",
        "\r\n"
      ]
    },
    {
      "name": "8515a895",
      "displayName": "YearsWithManager",
      "queryLines": [
        "SELECT \r\n",
        "       CASE WHEN HR.YearsWithCurrManager < 3 THEN '0-3'\r\n",
        "              WHEN HR.YearsWithCurrManager BETWEEN 3 AND 5 THEN '3-5'\r\n",
        "              WHEN HR.YearsWithCurrManager BETWEEN 5 AND 10 THEN '5-10'\r\n",
        "              WHEN HR.YearsWithCurrManager BETWEEN 10 AND 15 THEN '10-15'\r\n",
        "              WHEN HR.YearsWithCurrManager > 15 THEN '15+'\r\n",
        "              ELSE NULL END AS YearsWithCurrManager,\r\n",
        "       ROUND(\r\n",
        "                CAST(SUM(CASE WHEN HR.ATTRITION = 'Yes' THEN 1 ELSE 0 END) AS REAL) \r\n",
        "                / COUNT(*),\r\n",
        "        2) AttRatio\r\n",
        "       \r\n",
        "FROM workspace.default.ibm_hr HR\r\n",
        "GROUP BY YearsWithCurrManager\r\n",
        "\r\n",
        "\r\n"
      ]
    }
  ],
